{
  "name": "universalsnowpack",
  "type": "module",
  "version": "0.2.0",
  "description": "Example universal Javascript app using Snowpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/beejunk/universal-pika-example"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "snowpack build",
    "dev": "node --inspect scripts/start.js dev",
    "start": "node scripts/start.js",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "clean": "npm run clean:build && npm run clean:node",
    "clean:build": "rimraf build",
    "clean:node": "rimraf node_modules"
  },
  "keywords": [
    "javascript",
    "snowpack",
    "to-do"
  ],
  "author": "Brian David <beejunk@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "express": "^4.18.2",
    "htm": "^3.0.4",
    "lodash": "^4.17.20",
    "preact": "^10.5.10",
    "preact-render-to-string": "^5.1.12",
    "prop-types": "^15.7.2",
    "serialize-javascript": "^5.0.1",
    "snowpack": "^3.1.1"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  }
}
